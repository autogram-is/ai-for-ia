{
  "version": "0.1.1",
  "description": "Demo code for Rosenfeld AI for IA talk",
  "engines": {
    "node": "^22.5.1"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "schema:generate": "npx drizzle-kit generate",
    "schema:push": "npx drizzle-kit push",
    "initialize": "tsx src/scripts/initialize.ts",
    "embed": "tsx src/scripts/embed.ts",
    "nlp": "tsx src/scripts/nlp.ts",
    "locate": "tsx src/scripts/locate.ts",
    "locate:specialty": "tsx src/scripts/retrieval.ts",
    "epsilon": "tsx src/scripts/test-epsilon.ts",
    "ollama:check": "tsx src/scripts/ollama-check.ts",
    "position": "tsx src/scripts/position.ts",
    "compare": "tsx src/scripts/compare.ts",
    "cluster": "tsx src/scripts/cluster.ts",
    "describe": "tsx src/scripts/describe.ts",
    "prompt": "tsx src/scripts/prompt.ts",
    "prompt:test": "tsx src/scripts/test-prompts.ts",
    "report:clusters": "tsx src/scripts/report-clusters.ts",
    "report:movement": "tsx src/scripts/report-movement.ts",
    "build": "shx rm -rf dist; pkgroll --minify",
    "format": "prettier --config prettier.config.js 'src/**/*' --write",
    "lint": "eslint src/**/*",
    "lint-and-fix": "eslint src/**/* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autogram-is/rosenfeld-ai-for-ia"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@eslint/js": "^8",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint__js": "^8",
    "@types/node": "^22.5.4",
    "ava": "^6.1.3",
    "eslint": "^9",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5",
    "pkgroll": "^2.5",
    "prettier": "^3.2",
    "prettier-plugin-organize-imports": "^3.2",
    "shx": "^0.3.4",
    "tsx": "^4",
    "typescript": "^5.6",
    "typescript-eslint": "^8"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@cdxoo/dbscan": "^1.1.1",
    "@eatonfyi/ids": "github:eaton/ids",
    "@electric-sql/pglite": "^0.2.12",
    "@google/generative-ai": "^0.19.0",
    "@keckelt/tsne": "^1.0.2",
    "@nlpjs/basic": "^4.27.0",
    "@sindresorhus/is": "^7.0.1",
    "@types/cli-progress": "^3.11.6",
    "@types/pg": "^8.11.10",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "dbvis-hc": "^1.0.8",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.26.2",
    "drizzle-orm": "^0.35.3",
    "drizzle-zod": "^0.5.1",
    "emittery": "^1.0.3",
    "fs-jetpack": "^5.1.0",
    "mathjs": "^13.2.1",
    "ml-distance": "^4.0.1",
    "ml-kmeans": "^6.0.0",
    "ml-pca": "^4.1.1",
    "ollama": "^0.5.9",
    "openai": "^4.61.1",
    "p-memoize": "^7.1.1",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "sparkly": "^6.0.0",
    "tasuku": "^2.0.1",
    "zod": "^3.23.8"
  },
  "overrides": {
    "ajv": "^8.17.1",
    "whatwg-url": "^14.0.0"
  }
}
